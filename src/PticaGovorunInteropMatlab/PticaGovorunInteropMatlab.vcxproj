  <!-- From input Matlab *.m files builds the dll -->
  <Target Name="BuildMatlabProxy" BeforeTargets="Build" Inputs="@(None)" Outputs="$(IntDir)$(ProjectName).dll;$(OutDir)$(ProjectName).dll;">
    <Message Text="Building Matlab proxy" Importance="high" />
    <Message Text="MatlabDir=$(MatlabDir)" Importance="high" />
    <Message Text="BuildDir=$(IntDir)" Importance="high" />
    <Message Text="Matlab sources:" Importance="high" />
    <Message Text="%(None.FullPath)" Importance="high" />
    <!-- Construct list of *.m absolute pathes-->
    <ItemGroup>
      <MatlabMFileAbsPath Include="%(None.FullPath)" />
    </ItemGroup>
    <Exec Command="$(MatlabDir)bin\mcc -W cpplib:$(ProjectName) -T link:lib -d $(IntDir) -v @(MatlabMFileAbsPath, ' ')" />
    <Copy SourceFiles="$(IntDir)$(ProjectName).dll" DestinationFolder="$(OutDir)" SkipUnchangedFiles="True" />
  </Target>
